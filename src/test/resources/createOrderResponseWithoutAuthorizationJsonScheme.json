  "type": "object",
  "required": [
    "success",
    "name",
    "order"
  ],
  "properties": {
    "success": {
      "$id": "#/properties/success",
      "type": "boolean"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string"
    },
    "order": {
      "$id": "#/properties/order",
      "type": "object",
