{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "success",
    "user"
  ],
  "properties": {
    "success": {
      "$id": "#/properties/success",
      "type": "boolean"
    },
    "user": {
      "$id": "#/properties/user",
      "type": "object",
      "required": [
        "email",
        "name"
      ],
      "properties": {
        "email": {
          "$id": "#/properties/user/properties/email",
          "type": "string"
        },
        "name": {
          "$id": "#/properties/user/properties/name",
          "type": "string"
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}